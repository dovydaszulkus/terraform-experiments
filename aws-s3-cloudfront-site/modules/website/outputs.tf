output "website_domain_name" {
  description = "Domain name of the website bucket"
  value       = aws_s3_bucket.website.bucket_domain_name
}

output "website_arn" {
  description = "Resource ARN of the website bucket"
  value       = aws_s3_bucket.website.arn
}

output "test" {
  description = "Resource ARN of the website bucket"
  value       = "test"
}

output "website_url" {
  description = "Website URL generated by AWS Cloudfront service"
  value       = aws_cloudfront_distribution.website_cloudfront.domain_name
}

output "website_bucket_name" {
  description = "The name of the bucket that stores website files"
  value       = aws_s3_bucket.website.bucket
}
